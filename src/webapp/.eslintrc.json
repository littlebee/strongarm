{
  "env": {
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "no-case-declarations": "off",
    "no-prototype-builtins": "off",
    "react/jsx-fragments": "off",
    "react/jsx-handler-names": "off",
    "eslintjsx-a11y/anchor-is-valid": "off"
  },
  "globals": {
  }
}
